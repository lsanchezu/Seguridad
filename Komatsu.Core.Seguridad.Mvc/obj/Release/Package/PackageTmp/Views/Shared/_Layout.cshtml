<body>
    <div id="top">
        <div class="wrapper">
            <a href="#" title="" style="cursor: default" class="logo">
                
            </a>
            <div class="topNav">
                <ul class="userNav">
                    <li>
                        <a class="profile" title="" href="#">(@Komatsu.Core.Seguridad.Mvc.Helper.UtilSession.Obtener_UsuarioSession().UserName) 
                        - 
                            @Komatsu.Core.Seguridad.Mvc.Helper.UtilSession.Obtener_UsuarioSession().NombreApellido
                        - 
                        </a></li>
                    <li><a class="logout" title="" href="@Url.Action("CerrarSesion", "Cuenta")">Cerrar Sesión</a></li>
                </ul>
            </div>
            <div class="clear">
            </div>
        </div>
    </div>
    <div id="dialogAlert" style="display: none">
        <p id="buttonAlert">
        </p>
    </div>
    <div id="loading">
        <div id='loading-container'>
            <p id='loading-content'>
            </p>
        </div>
    </div>
    <div class="text_success">
        <div class="text_message">
            <img src='@Url.Content("~/Content/images/elements/loaders/7.gif")' alt="" />
            <span></span>
        </div>
    </div>
    <div id="sidebar" class="without">
        <div class="mainNav">
            @Html.MvcSiteMap().Menu(false, true, false)
        </div>
    </div>
    <div id="content" style="height: 100%; width: auto;">
        @RenderBody()
    </div>
</body>
</html>
<script type="text/javascript">
    $(window).load(function () {
        $("#loading").slideDown('slow', function () {
            $(this).remove();
            $('body').removeAttr('style');
        });
    });

    $("#menu").attr("class", "nav");
    var iconList = document.createElement("span");
    $("#menu li ul li a").each(function (indice, elemento) {
        var span = "<span class='icol-list'></span>";
        $(this).html(span + $(this).html());
    });

    $(".text_success").hide();
    $(".text_message").hide();

    $(".text_success").ajaxStart(function () {
        $(".text_message").show();
        $(this).show();
    })

    $(".text_success").ajaxComplete(function () {
        $(".text_message").hide();
        $(this).hide();
    });
</script>
